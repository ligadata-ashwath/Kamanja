{
  "clusterConfigFile": "{ClusterConfigFile}",
  "apiConfigFile": "{ApiConfigFile}",
  "unhandledMetadataDumpDir": "{UnhandledMetadataDumpDir}",
  "migratingFrom": {
    "version": "{KamanjaFromVersion}",
    "scalaVersion": "{ScalaFromVersion}",
    "versionInstallPath": "{OldPackageInstallPath}",
    "implemtedClass": "com.ligadata.Migrate.MigrateFrom_V_{KamanjaFromVersionWithUnderscore}",
    "jars": [
      "{NewPackageInstallPath}/lib/system/migratebase-1.4.0.jar",
      "{NewPackageInstallPath}/lib/system/migratefrom_v_{KamanjaFromVersionWithUnderscore}_{ScalaFromVersion}-1.4.0.jar",
      "{OldPackageInstallPath}/bin/KamanjaManager-1.0"
    ]
  },
  "migratingTo": {
    "version": "1.4",
    "scalaVersion": "{ScalaToVersion}",
    "versionInstallPath": "{NewPackageInstallPath}",
    "implemtedClass": "com.ligadata.Migrate.MigrateTo_V_1_4",
    "jars": [
      "{NewPackageInstallPath}/lib/system/migratebase-1.4.0.jar",
      "{NewPackageInstallPath}/lib/system/migrateto_v_1_4_{ScalaToVersion}-1.4.0.jar",
      "{NewPackageInstallPath}/lib/system/KamanjaInternalDeps_{ScalaToVersion}-1.4.0.jar",
      "{NewPackageInstallPath}/lib/system/ExtDependencyLibs_{ScalaToVersion}-1.4.0.jar",
      "{NewPackageInstallPath}/lib/system/ExtDependencyLibs2_{ScalaToVersion}-1.4.0.jar"
    ],
    "tenantId" : "{TenantId}",
    "adapterMessageBindings" : "{AdapterMessageBindings}"
  },
  "dataSaveThreshold": 2048,
  "parallelDegree": 16,
  "mergeContainersAndMessages": false,
  "excludeMetadata": [
    "JarDef",
    "OutputMsgDef"
  ],
  "excludeData": false
}

